{
  "semi": true,
  "singleQuote": false,
  "trailingComma": "all",
  "printWidth": 120,
  "endOfLine": "auto",
  "tabWidth": 2,
  "overrides": [
    {
      "files": ".prettierrc",
      "options": {
        "parser": "json"
      }
    },
    {
      "files": [
        "*.md"
      ],
      "options": {
        "embeddedLanguageFormatting": "off"
      }
    }
  ]
}